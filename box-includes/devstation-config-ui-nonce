gsettings set org.gnome.shell favorite-apps "['code.desktop', 'intellij-idea-community.desktop', 'meld.desktop', 'org.gnome.Terminal.desktop', 'org.gnome.Nautilus.desktop', 'google-chrome.desktop']" && gsettings set org.gnome.desktop.interface enable-animations false && gsettings set org.gnome.nautilus.desktop trash-icon-visible false && sudo mv /usr/local/bin/devstation-config-ui-nonce /usr/local/bin/devstation-config-ui
